public class TextManipulation {
    public static String squeeze(String input, String charactersToRemove) {
        StringBuilder result = new StringBuilder();

        for (int i = 0; i < input.length(); i++) {
            char c = input.charAt(i);
            if (charactersToRemove.indexOf(c) == -1) {
                result.append(c);
            }
        }

        return result.toString();
    }

    public static void main(String[] args) {
        String inputText = "Merhaba Dünya! Java Programlama İle Tanışın.";
        String charactersToRemove = "aeiouAEIOU"; // Örnek olarak sesli harfleri kaldırıyoruz.
        String degistirilmisMetin = squeeze(inputText, charactersToRemove);
        System.out.println("Değiştirilmiş Metin: " + degistirilmisMetin);
    }
}
